.MODEL SMALL
INCLUDE MACRO.ASM
.STACK 100H
.DATA
	DAT	DW	?
	WALK_COUNT		DB 0
.CODE
MAIN PROC
;STARTUP
	MOV AX,@DATA
	MOV DS,AX

	PUSH  DAT
	INC	WALK_COUNT
	mov	CL,10
	mov	AL,2
	SUB	 CL,AL
	MOV	 DL,CL
	MOV	 AH,2
	ADD	 DL,30H
loop1:
	int	21h
	
	mov	ah,1
	int	21h


;DOS EXIT
	MOV AH,4CH
	INT 21H
;EXIT
MAIN	ENDP
END		MAIN